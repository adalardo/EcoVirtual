\Name{compLV}

\Title{
Lotka-Volterra Competition Model	
}

\Description{
Simulate the Lotka-Volterra competition model for two populations.
}

\Usage{
compLV(n01,n02,tmax,r1,r2,k1,k2,alfa,beta)
}

\Arguments{
  \item{n01}{
    initial population for the best competitor species.
}
  \item{n02}{
    initial population for the worse competitor species.
}
  \item{tmax}{
    maximum simulation time.
}
  \item{r1}{
    intrinsic growth rate for the best competitor species.
}
  \item{r2}{
    intrinsic growth rate for the worse competitor species.
}
  \item{k1}{
    carrying capacity for the best competitor species.
}
  \item{k2}{
    carrying capacity for the worse competitor species.
}
  \item{alfa}{
    alfa coefficient.
}
  \item{beta}{
    beta coefficient
}
}

