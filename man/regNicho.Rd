\Name{regNicho}

\Title{
Succesional niche simulations
}

\Description{
Simulates the succession of niches in a regeneration process.
}

\Usage{
regNicho(tmax, rw, cl, c1, c2, ec, dst, er, sc, mx, rs)
}

\Arguments{
  \item{tmax}{
    maximum simulation time.
}
  \item{rw}{
    number of rows
}
  \item{cl}{
    number of columns
}
  \item{c1}{
    colonization rate for the superior competitor
}
  \item{c2}{
    colonization rate for the inferior competitor
}
  \item{ec}{
    competitive exclusion
}
  \item{dst}{
    disturbance
}
  \item{er}{
    early stage (only sp2)
}
  \item{sc}{
    susceptive stage (only sp1)
}
  \item{mx}{
    mixed (sp1 and sp2)
}
  \item{rs}{
    resistant
}
}

